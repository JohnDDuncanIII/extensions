content  moretools             content/
resource moretools             content/
skin     moretools classic/1.0 skin/

# Firefox main window
overlay chrome://browser/content/browser.xul                               chrome://moretools/content/moretools-overlay.xul

# Thunderbird main window and SeaMonkey mail window
overlay chrome://messenger/content/messenger.xul                           chrome://moretools/content/moretools-overlay.xul

# SeaMonkey browser
overlay chrome://navigator/content/navigator.xul                           chrome://moretools/content/moretools-overlay.xul

# Thunderbird and SeaMonkey mail composer
overlay chrome://messenger/content/messengercompose/messengercompose.xul   chrome://moretools/content/moretools-overlay.xul

# Thunderbird and SeaMonkey address book
overlay chrome://messenger/content/addressbook/addressbook.xul             chrome://moretools/content/moretools-overlay.xul

# SeaMonkey HTML editor
overlay chrome://editor/content/editor.xul                                 chrome://moretools/content/moretools-overlay.xul

override chrome://moretools/content/options.xul chrome://moretools/content/optionsLegacy.xul application={ec8030f7-c20a-464f-9b0e-13a3a9e97384} appversion<=6.*
override chrome://moretools/content/options.xul chrome://moretools/content/optionsLegacy.xul application={3550f703-e582-4d05-9a08-453d09bdfdc6} appversion<=6.*
override chrome://moretools/content/options.xul chrome://moretools/content/optionsLegacy.xul application={92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a} appversion<=2.3.*

